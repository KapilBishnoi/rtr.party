{
  "version": 2,
  "name": "rtr.party",
  "builds": [
    {
      "src": "*.css",
      "use": "@now/static"
    },
    {
      "src": "**/*.md",
      "use": "@now/md",
      "config": {
        "title": "राईट टू रिकॉल पार्टी",
        "language": "hi",
        "meta": [
          {
            "name": "description",
            "content": "Example markdown builder deployed to ZEIT NOW"
          }
        ],
        "css": "/style.css"
      }
    }
  ]
}
